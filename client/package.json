{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:go": "cd .. && GOOS=js GOARCH=wasm go build -o ./client/public/main.wasm . && cd client",
    "build": "yarn run build:go && vite build",
    "format": "biome format --write ./src",
    "lint": "biome lint --write ./src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/golang-wasm-exec": "^1.15.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.8"
  }
}
